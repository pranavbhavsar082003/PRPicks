@model List<PRList>


<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light box-shadow mb-3">
    <div class="container-fluid">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img
                src="https://i.pinimg.com/736x/eb/63/42/eb63428c369d5cdd886c980ddc55c81a.jpg" alt="image1"
                height="40px"></a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
            <ul class="navbar-nav flex-grow-1">
                @foreach (var PRList in Model)
                {
                    <li class="nav-item ">
                        <a class="nav-link" asp-controller="@PRList.Controller"
                            asp-action="@PRList.Action">@PRList.Label</a>
                    </li>
                }
                @if (User.Identity.IsAuthenticated)
                {
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-controller="Orders" asp-action="Index">Orders</a>
                    </li>
                }
            </ul>

            <ul class="navbar-nav">
                @if (User.Identity.IsAuthenticated)
                {
                    <li class="nav-item">
                        <a asp-area="Identity" asp-page="/Account/Manage/Index" class="nav-link text-dark">
                            Hello @User.Identity.Name!
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-area="Identity" asp-page="/Account/Logout" class="nav-link text-dark">
                            Logout
                        </a>
                    </li>
                }
                else
                {
                    <li class="nav-item">
                        <a asp-area="Identity" asp-page="/Account/Register" class="nav-link text-dark">
                            Register
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-area="Identity" asp-page="/Account/Login" class="nav-link text-dark">
                            Login
                        </a>
                    </li>
                }
            </ul>
        </div>
    </div>
</nav>